{
	"consumer": {
		"name": "NotesConsumer"
	},
	"provider": {
		"name": "NotesProvider"
	},
	"interactions": [
		{
			"description": "a paginated request for notes",
			"providerState": "notes exist",
			"request": {
				"method": "GET",
				"path": "/notes",
				"query": "page=1&perPage=10"
			},
			"response": {
				"status": 200,
				"headers": {
					"Content-Type": "application/json"
				},
				"body": {
					"notes": [
						{
							"id": 1,
							"content": "First note"
						},
						{
							"id": 2,
							"content": "Second note"
						}
					]
				}
			}
		}
	],
	"metadata": {
		"pactSpecification": {
			"version": "2.0.0"
		}
	}
}
