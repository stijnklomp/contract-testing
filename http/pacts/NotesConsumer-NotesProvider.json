{
  "consumer": {
    "name": "NotesConsumer"
  },
  "interactions": [
    {
      "description": "a paginated request for notes",
      "providerState": "notes exist",
      "request": {
        "matchingRules": {
          "$.query.page[0]": {
            "match": "type"
          },
          "$.query.perPage[0]": {
            "match": "type"
          }
        },
        "method": "GET",
        "path": "/notes",
        "query": "page=1&perPage=10"
      },
      "response": {
        "body": {
          "notes": [
            {
              "content": "Sample note content",
              "id": 1
            },
            {
              "content": "Sample note content",
              "id": 1
            }
          ]
        },
        "headers": {
          "Content-Type": "application/json"
        },
        "matchingRules": {
          "$.body.notes": {
            "match": "type",
            "min": 2
          },
          "$.body.notes[*].content": {
            "match": "type"
          },
          "$.body.notes[*].id": {
            "match": "type"
          }
        },
        "status": 200
      }
    }
  ],
  "metadata": {
    "pact-js": {
      "version": "15.0.1"
    },
    "pactRust": {
      "ffi": "0.4.22",
      "models": "1.2.3"
    },
    "pactSpecification": {
      "version": "2.0.0"
    }
  },
  "provider": {
    "name": "NotesProvider"
  }
}